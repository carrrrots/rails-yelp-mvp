<%= form_with model: @restaurant do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :address %>
  <%= form.text_field :address %>
  <%= form.label :phone_number %>
  <%= form.number_field :phone_number %>
  <%= form.label :category %>
  <%= form.collection_select :category %>
  <%= form.submit %>
<% end %>
